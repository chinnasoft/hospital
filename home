    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        
         Runtime runtime = Runtime.getRuntime();
            try {

                String folderPath = "D:/EHOUSE/dataBackUp";

                File f1 = new File(folderPath);
                f1.mkdir();
                java.util.Date date5 = new java.util.Date();
        // SimpleDateFormat sec = new SimpleDateFormat("dd-MMM-yyyy::::kk:mm:ss");
        SimpleDateFormat sec = new SimpleDateFormat("EEE");
        String timesec = sec.format(date5);



                // String savePath = "D:/dataBackUp/" + timesec + ".sql";

                SimpleDateFormat dd = new SimpleDateFormat("dd-MMM-yyyy");
                String timesec1 = dd.format(date5);

                String s = "TC" + timesec1;

              //  Process runtimeProcess;

             //   runtimeProcess = runtime.exec("cmd /start /c C:\\Program Files\\MySQL\\MySQL Server 6.0\\bin\\mysqldump -uroot -padmin joseph> D:/EHOUSE/dataBackUp/" + s + ".sql");

           //runtimeProcess = runtime.exec("cmd /start /cd C:/Program Files/MySQL/MySQL Server 6.0/bin/mysqldump -uroot -padmin joseph> D:/EHOUSE/dataBackUp/TC19-Feb-2016.sql");
        
               
                Process p1 = Runtime.getRuntime().exec("D:\\EHOUSE\\dist\\lib\\class\\sp\\rar\\ex.bat");
                
                 int processComplete = p1.waitFor();

                 if (processComplete == 0) {
                    // System.out.println("Backup restored successfully");
                    JOptionPane.showMessageDialog(null, "Successfully Back up database");
                } else {
                    System.out.println("Could not restore the backup" + processComplete);
                }

            } catch (Exception ex) {
                System.out.println(ex);
            }
    }                                        

    void getNormal() {

       // jButton2.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/PERSONAL.png"))); // NOI18N
      //  jButton14.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/DEALER.png"))); // NOI18N   
      //  jButton13.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/CUSTOMER.png"))); // NOI18N
     //   jButton5.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/STOCK.png"))); // NOI18N
      //  jButton3.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/SALES BILL.png")));
      //  jButton7.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/ORDER_1.png"))); // NOI18N
      //  jButton6.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/EXPENSE.png"))); // NOI18N
      //  jButton11.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/DEPOSIT.png"))); // NOI18N
      //  jButton1.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/STOCK REPORT.png"))); // NOI18N                 
       // jButton4.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/SALES REPORT.png"))); // NOI18N       
        jButton10.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/NOTES.png"))); // NOI18N
        jButton8.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/calc.png"))); // NOI18N
        jButton9.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/CONTACTS.png"))); // NOI18N

        String x = jLabel1.getText();
        if (x.equals("DL")) {
         //   jButton14.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/DEALER.jpg"))); // NOI18N   
        } else if (x.equals("CS")) {
        //    jButton13.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/CUSTOMER.jpg"))); // NOI18N
        } else if (x.equals("SC")) {
       //     jButton5.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/STOCK.jpg"))); // NOI18N  
        } else if (x.equals("SB")) {
         //   jButton3.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/SALES BILL.jpg")));
        } else if (x.equals("SR")) {
        //    jButton7.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/ORDER.jpg"))); // NOI18N
        } else if (x.equals("SP")) {
        //    jButton1.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/STOCK REPORT.jpg"))); // NOI18N  
        } else if (x.equals("LP")) {
       //     jButton4.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/SALES REPORT.jpg"))); // NOI18N 
        } else if (x.equals("RP")) {
       //     jButton2.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/PERSONAL.jpg"))); // NOI18N   
        } else if (x.equals("EP")) {
         //   jButton6.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/EXPENSE.jpg"))); // NOI18N
        } else if (x.equals("BK")) {
         //   jButton11.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/DEPOSIT.jpg"))); // NOI18N
        } else if (x.equals("calc")) {
            jButton8.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/calc.jpg"))); // NOI18N
        } else if (x.equals("cts")) {
            jButton9.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/CONTACTS.jpg"))); // NOI18N 
        } else if (x.equals("notes")) {
            jButton10.setIcon(new javax.swing.ImageIcon(getClass().getResource("/images/NOTES.jpg"))); // NOI18N
        }
    }

